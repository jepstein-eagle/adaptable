{
  "name": "@adaptabletools/adaptable-ng-aggrid",
  "version": "0.0.0",
  "publishConfig": {
    "registry": "https://registry.adaptabletools.com"
  },
  "description": "Angular version of the powerful data-agnostic HTML5 datagrid add-on that sits on top of the agGrid component and provides all the rich functionality that advanced users expect from their DataGrids and Data Tables",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaptableTools/adaptable.git",
    "directory": "packages/adaptable-ng-aggrid"
  },
  "bugs": {
    "url": "https://github.com/AdaptableTools/adaptable/issues"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@ag-grid-community/all-modules": "23.2.0",
    "@ag-grid-community/angular": "23.2.0",
    "@ag-grid-enterprise/menu": "23.2.0",
    "@ag-grid-enterprise/all-modules": "23.2.0",
    "@ag-grid-enterprise/range-selection": "23.2.0",
    "@ag-grid-enterprise/side-bar": "23.2.0",
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular-devkit/build-ng-packagr": "~0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "^9.1.7",
    "@angular/compiler-cli": "^9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/language-service": "~9.1.7",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.60",
    "chalk": "^3.0.0",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^9.1.3",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "protractor": "~5.4.0",
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "rimraf": "^2.6.2",
    "rxjs": "~6.5.5",
    "symlink-dir": "^3.1.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.38.1",
    "tslint": "~5.15.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "ng": "ng",
    "link-core": "symlink-dir ../adaptable-parser ./node_modules/@adaptabletools/adaptable-parser && symlink-dir ../adaptable ./node_modules/@adaptabletools/adaptable && npm run --prefix ./projects/adaptable-angular-aggrid link-core",
    "start": "npm run link-core && node --max-old-space-size=4096 ./node_modules/.bin/ng serve",
    "clean-deps": "rimraf yarn.lock && rimraf package-lock.json && rimraf node_modules && npm i && npm run clean-deps-projects",
    "clean-deps-projects": "rimraf projects/adaptable-angular-aggrid/yarn.lock && rimraf projects/adaptable-angular-aggrid/package-lock.json && rimraf projects/adaptable-angular-aggrid/node_modules && npm --prefix projects/adaptable-angular-aggrid/ i",
    "build": "npm run clean-deps && rimraf dist && npm run build-prod-no-cleanup",
    "build-prod-no-cleanup": "npm run before-build && npm run link-core && node --max-old-space-size=4096 ./node_modules/.bin/ng build adaptable-angular-aggrid && npm run build-package-json && npm run sass && npm run copy-assets && npm run after-build",
    "before-build": "npm run ensure-clean-on-local",
    "after-build": "",
    "ensure-clean-on-local": "node ../../scripts/ensure-clean-on-local",
    "test": "ng test",
    "lint": "ng lint",
    "tsc": "npm run link-core && tsc",
    "e2e": "ng e2e",
    "pretty": "prettier --write \"packages/**/*.{js,jsx,ts,tsx,json,css,scss}\" --fix",
    "registry-publish": "npm run build-package-json && npm publish dist/adaptable-angular-aggrid",
    "registry-publish-canary": "npm run build-package-json &&  npm publish --tag canary dist/adaptable-angular-aggrid",
    "build-package-json": "node ../../scripts/cli/update-package --module=packages/adaptable-ng-aggrid/dist/adaptable-angular-aggrid --dist-folder=packages/adaptable-ng-aggrid/dist/adaptable-angular-aggrid/",
    "update-version-file": "node ../../scripts/updateVersionFile",
    "update-env-file": "node ../../scripts/updateEnvFile",
    "sass": "node-sass ./src/index.scss ./dist/adaptable-angular-aggrid/index.css && ../../node_modules/.bin/postcss ./dist/adaptable-angular-aggrid/index.css --config ../../ --output ./dist/adaptable-angular-aggrid/index.css && node-sass ./src/base.scss ./dist/adaptable-angular-aggrid/base.css && ../../node_modules/.bin/postcss ./dist/adaptable-angular-aggrid/base.css --config ../../ --output ./dist/adaptable-angular-aggrid/base.css && node-sass ./src/themes/light.scss ./dist/adaptable-angular-aggrid/themes/light.css && ../../node_modules/.bin/postcss ./dist/adaptable-angular-aggrid/themes/light.css --config ../../ --output ./dist/adaptable-angular-aggrid/themes/light.css && node-sass ./src/themes/dark.scss ./dist/adaptable-angular-aggrid/themes/dark.css && ../../node_modules/.bin/postcss ./dist/adaptable-angular-aggrid/themes/dark.css --config ../../ --output ./dist/adaptable-angular-aggrid/themes/dark.css",
    "copy-assets": "cp .npmignore ./dist/adaptable-angular-aggrid && cp -R ../adaptable/src/fonts ./dist/adaptable-angular-aggrid/fonts/ && cp README.md ./dist/adaptable-angular-aggrid/"
  }
}
