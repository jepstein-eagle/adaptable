{
  "version": "0.1.0",
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx test --watch",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "build": "node ./src/generate.js",
    "prepare": "npm run build",
    "build-prod-no-cleanup": "npm run before-build && npm run tsc && npm run build-package-json && npm run after-build",
    "build-package-json": "node ../../scripts/cli/update-package --module=packages/adaptable-parser --ignore-dep=true",
    "before-build": "npm run ensure-clean-on-local",
    "after-build": "",
    "ensure-clean-on-local": "node ../../scripts/ensure-clean-on-local",
    "tsc": "rimraf dist && tsc --outDir dist/src && cpy src/parser.js dist/src",
    "registry-publish": "npm run build-package-json && npm publish dist",
    "registry-publish-canary": "npm run build-package-json && npm publish --tag canary dist"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@adaptabletools/adaptable-parser",
  "publishConfig": {
    "registry": "https://registry.adaptabletools.com"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "cpy-cli": "^3.1.1",
    "husky": "^4.2.3",
    "jison": "^0.4.18",
    "rimraf": "^3.0.2",
    "tsdx": "^0.13.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
